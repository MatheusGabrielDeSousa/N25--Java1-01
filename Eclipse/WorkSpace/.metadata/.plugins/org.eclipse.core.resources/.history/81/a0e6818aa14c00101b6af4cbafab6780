package br.com.senaisp.bauru.aula06;

import java.util.Scanner;

public class Exemplo03 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		//Criando uma matriz
		//Cinema com 5 fileiras e 10 cadeiras por fileira//char=caracter []=conjunto
		char[][] cinema = new char[5][9];
		//Inicializando com "_"
		for(int i=0; i<cinema.length; i++) {
		  //Preenchendo as cadeiras
			for	(int j=0; j<cinema.length; j++) {
				cinema [i][j] = '_';
			}
		}
		//Iniciando o programa
		String continua;
		do {
			// mostrando os acentos
		}while(continua.charAt(0)!='S');
	}

}
